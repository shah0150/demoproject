<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.abcd0001.myApp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Adesh and Prabhjot</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
          </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    <splash src="res/screen/ios/logo.png" width="320" height="480"/>
    <splash src="res/screen/ios/logo.png" width="640" height="960"/>
    <splash src="res/screen/ios/logo.png" width="768" height="1024"/>
    <splash src="res/screen/ios/logo.png" width="1536" height="2048"/>
    <splash src="res/screen/ios/logo.png" width="1024" height="768"/>
    <splash src="res/screen/ios/logo.png" width="2048" height="1536"/>
    <splash src="res/screen/ios/logo.png" width="640" height="1136"/>
    <splash src="res/screen/ios/logo.png" width="750" height="1334"/>
    <splash src="res/screen/ios/logo.png" width="1242" height="2208"/>
    <splash src="res/screen/ios/logo.png" width="2208" height="1242"/>
    </platform>
    <preference name="SplashScreen" value="screen"/>
     

    
</widget>
